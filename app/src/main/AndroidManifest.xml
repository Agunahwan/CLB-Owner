<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.agunahwanabsin.sitlowner">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_clb"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_clb_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.kotak.DetailPengecekanActivity"></activity>
        <activity android:name=".ui.kotak.ObjectActivity" />
        <activity android:name=".ui.kotak.TanggalPengecekanActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SplashScreenActivity"
            android:noHistory="true"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>